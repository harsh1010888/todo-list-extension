{
  "manifest_version": 3,
  "name": "Harsh's Todo List",
  "description": "A beautiful, AMOLED, animated, multi-list todo extension.",
  "version": "1.0",
  "icons": {
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "todo.html",
    "default_title": "Harsh's Todo List"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}